{
  "name": "sices-frontend-client",
  "description": "Sices Platform - Web Frontend Client",
  "author": "Kolina Labs",
  "private": true,
  "scripts": {
    "build": "webpack --config .webpack.prod.conf.js",
    "start": "npm run start:dev",
    "start:dev": "webpack-dev-server --hot --config .webpack.dev.conf.js",
    "lint": "npm run lint:template; npm run lint:style; npm run lint:script",
    "lint:template": "pug-lint-vue ./src",
    "lint:style": "stylelint '**/*.vue' --syntax scss",
    "lint:script": "eslint --ext .js,.vue ./src"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.9",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-vue": "^4.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.10",
    "postcss-url": "^7.3.0",
    "pug": "^2.0.0-rc.4",
    "pug-lint": "^2.5.0",
    "pug-lint-vue": "^0.1.3",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-processor-html": "^1.0.0",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.7.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "vue": "^2.5.13",
    "vue-awesome": "^2.3.4",
    "vue-axios": "^2.0.2",
    "vue-router": "^3.0.1"
  }
}
