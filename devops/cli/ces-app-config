#!/usr/bin/env bash

cd $SICES_PATH

echo "Starting App configurations..."

$CLI_PATH/util/variables-replacer

if [ -z ${1+x} ] || [ $1 = '--development' ]; then

  cp $CLI_PATH/util/pre-push .git/hooks/pre-push
  $CLI_PATH/ces-database-config --development

else

  mkdir .temp
  mkdir .mirror
  mkdir .backup

fi
