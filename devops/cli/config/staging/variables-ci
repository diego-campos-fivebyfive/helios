#!/usr/bin/env bash

source $CLI_PATH/config/staging/variables

export CES_SICES_PORT=$SICES_STAGING_PORT
export CES_SICES_HOST=$SICES_STAGING_HOST
export CES_SICES_DATABASE_NAME=$SICES_STAGING_DATABASE_NAME
export CES_SICES_DATABASE_USER=$SICES_STAGING_DATABASE_USER
export CES_SICES_DATABASE_HOST=$SICES_STAGING_DATABASE_HOST
export CES_SICES_DATABASE_PORT=$SICES_STAGING_DATABASE_PORT
export CES_SICES_DATABASE_PASS=$SICES_STAGING_DATABASE_PASS

export CES_BUNDLE_PORT=$BUNDLE_STAGING_PORT
export CES_BUNDLE_HOST=$BUNDLE_STAGING_HOST

export CES_ISQUIK_PORT=$ISQUIK_PRODUCTION_SERVER_PORT
export CES_ISQUIK_HOST=$ISQUIK_PRODUCTION_SERVER_HOST
export CES_ISQUIK_AUTH_KEY=$ISQUIK_PRODUCTION_AUTH_KEY
export CES_ISQUIK_AUTH_USER=$ISQUIK_PRODUCTION_AUTH_USER
