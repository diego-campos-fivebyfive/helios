export CES_SICES_DATABASE_PASS=$SICES_PRODUCTION_DATABASE_PASS
export CES_SICES_FTP_HOST=$SICES_PRODUCTION_FTP_HOST
export CES_SICES_FTP_USER=$SICES_PRODUCTION_FTP_USER
export CES_SICES_FTP_PASS=$SICES_PRODUCTION_FTP_PASS
