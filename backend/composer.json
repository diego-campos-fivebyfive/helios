{
    "name": "claudinei/sol.rm",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.3.9",
        "apy/breadcrumbtrail-bundle": "^1.4",
        "aws/aws-sdk-php-symfony": "^1.3",
        "doctrine/doctrine-bundle": "~1.4",
        "doctrine/orm": "^2.4.8",
        "friendsofsymfony/oauth-server-bundle": "dev-master",
        "friendsofsymfony/rest-bundle": "^2.2",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "gaufrette/phpseclib-sftp-adapter": "^0.4.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "jms/serializer-bundle": "^1.1",
        "knplabs/doctrine-behaviors": "~1.1",
        "knplabs/knp-gaufrette-bundle": "~0.3",
        "knplabs/knp-menu-bundle": "^2.0",
        "knplabs/knp-paginator-bundle": "^2.5",
        "knplabs/knp-snappy-bundle": "~1.4",
        "kolinalabs/customer-bundle": "dev-master",
        "liuggio/excelbundle": "^2.1",
        "nelmio/api-doc-bundle": "^2.13",
        "nelmio/cors-bundle": "^1.5",
        "oneup/uploader-bundle": "~1.4",
        "phpoffice/phpspreadsheet": "^1.2",
        "phpoffice/phpword": "v0.14.*",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "sonata-project/classification-bundle": "^3.1",
        "sonata-project/core-bundle": "^3.2",
        "sonata-project/doctrine-extensions": "^1.0",
        "sonata-project/intl-bundle": "2.2.4",
        "sonata-project/media-bundle": "^3.5",
        "symfony/assetic-bundle": "^2.8",
        "symfony/monolog-bundle": "~2.4",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/symfony": "2.8.20",
        "vindi/vindi-php": "dev-master"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "liip/functional-test-bundle": "^1.6",
        "phpunit/phpunit": "5.5.*",
        "sensio/generator-bundle": "~3.0",
        "symfony/phpunit-bridge": "~2.7"
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap"
        ]
    }
}
