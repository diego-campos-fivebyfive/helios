<table class="table table-bordered table-hover">
    <thead>
    <tr>
        <th class="col-md-4"><i class="fa fa-sort-desc"></i> Nome da Tag </th>
        <th class="col-md-8"><i class="fa fa-sort-desc"></i> Descrição da Tag </th>
    </tr>
    </thead>
    <tbody>
        {% for item in tagsList %}
            <tr>
                <td>{{ item.tag }}</td>
                <td>{{ item.description }}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>
