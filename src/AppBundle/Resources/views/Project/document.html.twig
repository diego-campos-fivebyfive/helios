<html>
<head>

    {% set sections = settings.sections %}
    {% set cover_image = settings.cover_image %}
    {% set header_logo = settings.header_logo %}
    {% set header_text = settings.header_text %}
    {% set fixed_sections = settings.fixed_sections %}
    {% set section_title_background = settings.section_title_background %}
    {% set section_title_font_family = settings.section_title_font_family %}

    <style>
        body{
            /*font-family:arial;*/
            font-size:10pt;
        }
        #header_info{
            font-size: 8pt;
            line-height: 12pt;
        }
        .table_section{
            width:100%;
            font-size:8pt;
            margin-top:20pt;
        }
        .table_section thead th{
            background:{{ section_title_background }};
            color:#ffffff;
            text-align:left;
            text-transform:uppercase;
            padding: 3pt;
            font-weight:normal;
        }
        .table_section thead.no_bg th{
            background:none;
            color:#222222;
            text-transform:none;
        }
        .table_section tbody tr th{
            text-align: right;
        }
        .table_section tbody tr td{
            padding-left: 10pt;
        }
        .extra_info tbody td{
            line-height:15pt;
        }
        .bolder{
            font-weight:bolder;
        }
        .underline{
            text-decoration:underline;
        }
        #info_box{
            line-height: 20pt;
            border:1pt solid #e68d24;
        }
        #info_box span.bolder{
            font-size:45pt;
        }
    </style>
</head>
<body>

{% macro _header(logo, text) %}

    <table width="100%">
        <tr>
            <td>
                {% if logo is not null %}
                    <img src="{{ logo }}" style="height:75pt;">
                {% endif %}
            </td>
            <td id="header_info" align="right">
                {{ text|raw }}
            </td>
        </tr>
    </table>

{% endmacro %}
{% import _self as doc %}


{% if cover_image is not null %}

    <pageheader>
        <img src="{{ cover_image }}" style="width:210mm; height:297mm;">
    </pageheader>

{% endif %}

{% if cover_image is not null %}

    <sethtmlpageheader name="defaultHeader">
    </sethtmlpageheader>

    <formfeed></formfeed>

    <htmlpageheader name="defaultHeader">
        {{ doc._header(header_logo, header_text) }}
    </htmlpageheader>

{% else %}

    <pageheader>
        {{ doc._header(header_logo, header_text) }}
    </pageheader>

    <htmlpageheader name="pageHeader">
        {{ doc._header(header_logo, header_text) }}
    </htmlpageheader>

    <sethtmlpageheader name="pageHeader"></sethtmlpageheader>

{% endif %}

{% for section in sections %}

    {% if attribute(fixed_sections, section.content) is defined %}

        {% if section.content == '[fixed_section][customer_data]' %}
            {% if project.customer %}
                <table class="table_section" cellpadding="3" cellspacing="0">
                    <thead>
                    <tr>
                        <th colspan="4">
                            <font face="{{ section_title_font_family }}"> Dados do cliente </font>
                        </th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr>
                        <th width="20%"> Nome:</th>
                        <td> {{ project.customer.name }} </td>
                        <th> Telefone:</th>
                        <td> {{ project.customer.phone }} </td>
                    </tr>
                    <tr>
                        <th width="20%"> Cidade:</th>
                        <td> {{ project.customer.city }} - {{ project.customer.state }} </td>
                        <th> Email:</th>
                        <td> {{ project.customer.email }} </td>
                    </tr>
                    </tbody>
                </table>
            {% endif %}
        {% endif %}


        {% if section.content == '[fixed_section][consumption_data]' %}
            <h3> Dados de Consumo [Coletar] </h3>
        {% endif %}

        {% if section.content == '[fixed_section][graph_generation]' %}
            <h3> Gráfico de Consumo </h3>
            <div style="width:100%">
                <img src="{{ project.calculationChart }}"/>
            </div>
        {% endif %}

        {% if section.content == '[fixed_section][financial_analysis]' %}
            <h3> Análise Financeira </h3>
        {% endif %}

        {% if section.content == '[fixed_section][graph_financial]' %}
            <h3> Gráfico Financeiro </h3>
        {% endif %}

        {% if section.content == '[fixed_section][project_items]' %}
            <table class="table_section" cellpadding="3" cellspacing="0">
                <thead>
                <tr>
                    <th width="10%"> Item</th>
                    <th> Descrição</th>
                    <th width="15%"> Potência</th>
                    <th width="15%"> Valor</th>
                </tr>
                </thead>
                <tbody>
                {% for i in 1..5 %}
                    <tr>
                        <td> {{ i }} </td>
                        <td> Description for {{ i }} </td>
                        <td> 9.6 kWh</td>
                        <td> R$ {{ i * 977.54 }} </td>
                    </tr>
                {% endfor %}
                </tbody>
                <tfoot>
                <tr>
                    <td>&nbsp;</td>
                    <th align="left"> Total</th>
                    <td>&nbsp;</td>
                    <th> R$ 55887.55</th>
                </tr>
                </tfoot>
            </table>
        {% endif %}

    {% else %}

        <table class="table_section extra_info" cellpadding="3" cellspacing="0">
            <thead>
            <tr>
                <th style="font-familiy:arial"> {{ section.title|raw }} </th>
            </tr>
            </thead>
            <tbody>
            <tr>
                <td>
                    {{ section.content|raw }}
                </td>
            </tr>
            </tbody>
        </table>

    {% endif %}

{% endfor %}

